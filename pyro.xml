<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default revision="pyro" sync-j="4" sync-c="true"/>


  <remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
  <remote fetch="https://github.com/rtavs" name="rtavs"/>
  <remote fetch="https://github.com/openembedded" name="oe"/>
  <remote fetch="https://github.com/OSSystems" name="ossys"/>
  <remote fetch="https://github.com/meta-qt5" name="QT5"/>
  <remote fetch="https://github.com/rockchip-linux" name="rockchip"/>


  <project remote="yocto" revision="pyro" name="poky" path="sources/poky"/>
  <project remote="oe" revision="pyro" name="meta-openembedded" path="sources/meta-openembedded"/>
  <project remote="rtavs" revision="tunus" name="meta-qt5" path="sources/meta-qt5" />

  <project remote="rockchip" revision="pyro" name="meta-rockchip" path="sources/meta-rockchip"/>
  <project remote="rockchip" revision="pyro" name="meta-rockchip-extra" path="sources/meta-rockchip-extra"/>

  <project remote="rockchip" revision="master" name="rkbin" path="rkbin"/>

  <project remote="rockchip" revision="yocto" name="build" path="sources/base">
        <copyfile dest="README" src="README"/>
        <copyfile dest="setup-environment" src="setup-environment"/>
  </project>

</manifest>
